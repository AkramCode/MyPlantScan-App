{
    "expo":  {
                 "name":  "MyPlantScan",
                 "slug":  "myplantscan",
                 "version":  "1.0.0",
                 "orientation":  "portrait",
                 "icon":  "./assets/images/icon.png",
                 "scheme":  "myplantscan",
                 "userInterfaceStyle":  "automatic",
                 "newArchEnabled":  true,
                 "splash":  {
                                "image":  "./assets/images/icon.png",
                                "resizeMode":  "contain",
                                "backgroundColor":  "#F9FAFB"
                            },
                 "ios":  {
                             "supportsTablet":  true,
                             "bundleIdentifier":  "com.myplantscan.www",
                             "infoPlist":  {
                                               "NSCameraUsageDescription":  "Allow $(PRODUCT_NAME) to access your camera",
                                               "NSMicrophoneUsageDescription":  "Allow $(PRODUCT_NAME) to access your microphone",
                                               "NSPhotoLibraryUsageDescription":  "Allow $(PRODUCT_NAME) to access your photos",
                                               "CFBundleURLTypes":  [
                                                                        {
                                                                            "CFBundleURLName":  "myplantscan",
                                                                            "CFBundleURLSchemes":  [
                                                                                                       "myplantscan"
                                                                                                   ]
                                                                        }
                                                                    ]
                                           },
                             "associatedDomains":  [
                                                       "applinks:myplantscan.com",
                                                       "applinks:www.myplantscan.com"
                                                   ]
                         },
                 "android":  {
                                 "adaptiveIcon":  {
                                                      "foregroundImage":  "./assets/images/adaptive-icon.png",
                                                      "backgroundColor":  "#F9FAFB"
                                                  },
                                 "package":  "com.myplantscan.www",
                                 "permissions":  [
                                                     "CAMERA",
                                                     "RECORD_AUDIO",
                                                     "READ_EXTERNAL_STORAGE",
                                                     "WRITE_EXTERNAL_STORAGE"
                                                 ],
                                 "intentFilters":  [
                                                       {
                                                           "category":  [
                                                                            "BROWSABLE",
                                                                            "DEFAULT"
                                                                        ],
                                                           "data":  [
                                                                        {
                                                                            "scheme":  "myplantscan"
                                                                        }
                                                                    ],
                                                           "action":  "VIEW"
                                                       },
                                                       {
                                                           "category":  [
                                                                            "BROWSABLE",
                                                                            "DEFAULT"
                                                                        ],
                                                           "data":  [
                                                                        {
                                                                            "scheme":  "https",
                                                                            "host":  "myplantscan.com",
                                                                            "pathPrefix":  "/auth"
                                                                        },
                                                                        {
                                                                            "scheme":  "https",
                                                                            "host":  "myplantscan.com",
                                                                            "pathPrefix":  "/reset-password"
                                                                        },
                                                                        {
                                                                            "scheme":  "https",
                                                                            "host":  "myplantscan.com",
                                                                            "pathPrefix":  "/"
                                                                        },
                                                                        {
                                                                            "scheme":  "https",
                                                                            "host":  "www.myplantscan.com",
                                                                            "pathPrefix":  "/auth"
                                                                        },
                                                                        {
                                                                            "scheme":  "https",
                                                                            "host":  "www.myplantscan.com",
                                                                            "pathPrefix":  "/reset-password"
                                                                        },
                                                                        {
                                                                            "scheme":  "https",
                                                                            "host":  "www.myplantscan.com",
                                                                            "pathPrefix":  "/"
                                                                        }
                                                                    ],
                                                           "action":  "VIEW",
                                                           "autoVerify":  true
                                                       }
                                                   ]
                             },
                 "web":  {
                             "favicon":  "./assets/images/favicon.png"
                         },
                 "plugins":  [
                                 [
                                     "expo-router",
                                     {
                                         "origin":  "https://www.myplantscan.com/"
                                     }
                                 ],
                                 [
                                     "expo-camera",
                                     {
                                         "cameraPermission":  "Allow $(PRODUCT_NAME) to access your camera",
                                         "microphonePermission":  "Allow $(PRODUCT_NAME) to access your microphone",
                                         "recordAudioAndroid":  true
                                     }
                                 ],
                                 [
                                     "expo-image-picker",
                                     {
                                         "photosPermission":  "The app accesses your photos to let you share them with your friends."
                                     }
                                 ],
                                 [
                                     "react-native-edge-to-edge",
                                     {
                                         "android":  {
                                                         "edgeToEdgeEnabled":  true
                                                     }
                                     }
                                 ]
                             ],
                 "experiments":  {
                                     "typedRoutes":  true
                                 }
             }
}
